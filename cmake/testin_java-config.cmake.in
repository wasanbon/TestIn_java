# TestIn_java CMake config file
#
# This file sets the following variables:
# TestIn_java_FOUND - Always TRUE.
# TestIn_java_INCLUDE_DIRS - Directories containing the TestIn_java include files.
# TestIn_java_IDL_DIRS - Directories containing the TestIn_java IDL files.
# TestIn_java_LIBRARIES - Libraries needed to use TestIn_java.
# TestIn_java_DEFINITIONS - Compiler flags for TestIn_java.
# TestIn_java_VERSION - The version of TestIn_java found.
# TestIn_java_VERSION_MAJOR - The major version of TestIn_java found.
# TestIn_java_VERSION_MINOR - The minor version of TestIn_java found.
# TestIn_java_VERSION_REVISION - The revision version of TestIn_java found.
# TestIn_java_VERSION_CANDIDATE - The candidate version of TestIn_java found.

message(STATUS "Found TestIn_java-@PROJECT_VERSION@")
set(TestIn_java_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(TestIn_java_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(TestIn_java_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(TestIn_java_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(TestIn_java_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(TestIn_java_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(TestIn_java_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(TestIn_java_DEFINITIONS ${<dependency>_DEFINITIONS})

set(TestIn_java_VERSION @PROJECT_VERSION@)
set(TestIn_java_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(TestIn_java_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(TestIn_java_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(TestIn_java_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

